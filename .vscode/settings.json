{
  "todo-tree.filtering.excludeGlobs": ["**/*.md"],
  "todo-tree.general.tags": [
    "BUG",
    "FIXME",
    "NOTE",
    "TODO",
    "WRN",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check-circle-fill",
      "foreground": "green",
      "iconColour": "green"
    },
    "FIXME": {
      "icon": "flame",
      "foreground": "yellow",
      "iconColour": "yellow"
    },
    "NOTE": {
      "icon": "paintbrush",
      "foreground": "#00bfff",
      "iconColour": "#00bfff"
    },
    "WRN": {
      "icon": "alert-fill",
      "foreground": "orange",
      "iconColour": "orange"
    },
    "BUG": {
      "icon": "bug",
      "foreground": "red",
      "iconColour": "red"
    }
  },
  "jest.coverageColors": {
    "uncovered": "rgba(255, 99, 71, 0.7)",
    "partially-covered": "rgba(255, 215, 0, 0.7)",
    "covered": "rgba(50, 205, 50, 0.7)"
  },
  "jest.jestCommandLine": "node ./node_modules/jest/bin/jest.js --config=jest.all.config.js",
  "jest.runMode": {
    "type": "on-demand"
  }
}
